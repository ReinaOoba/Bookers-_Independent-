<div class="container　px-5 px-sm-0">
  <div class='row'>
<!--左テンプレート-->
    <div class='col-md-3'>
      <%= render "user_info", user: current_user %>
    </div>
<!--ユーザー一覧-->
    <div class='col-md-8 offset-md-1'>
      <h1>Users</h1>
      <% @users.each do |user| %>
      <table class="table">
        <thead>
          <tr>
            <th>image</th>
            <th>name</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <th><%= user.get_profile_image(80,80) %></th>
            <th><%= user.name %></th>
            <th><%= link_to "Show", user_path(user)%></th>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>
